<%-include("partials/header"); -%>

    <h1>Write..</h1>
    <form name="" method="POST" action="/compose/<%=items%>">
        <div class="form-group">
            <label name="postTopic">Topic</label>
            <input class="form-control" type="text" name="postTopic" value="Music" readonly>
            <label name="postTitle">Title</label>
            <input class="form-control" type="text" name="postTitle">
            <label name="postarticle">Post</label>
            <textarea class="form-control" name="postarticle" rows="8" cols="80"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

    <%-include("partials/footer"); -%>